<div class="container">
<div class="page-header">
<h1><center><a href="/dashboard/<%= @user.name%>"> AirBnB Dashboard</a></center></h1>
</div>

<div class="row">
<div class="col-md-6">

<h3>Your Property</h3>
	

<table class="table">
<tr>
<th>Title:</th>
<td><%= @property.title %> </td>
</tr>
<tr>
<th>Price (RM):</th>
<td><%= @property.price %> </td>
</tr>
<tr>
<th>Address:</th>
<td><%= @property.address %> </td>
</tr>
<tr>
<th>Description:</th>
<td><%= @property.description %> </td>
</tr>
<tr>
<th>Tags:</th>
<td><%= @property.tags.pluck(:tag).join(", ") %> </td>
</tr>
</table>




<center>	
<a href="/property/edit/<%= @property.id %>"><button class="btn btn-default">Update Property</button></a>
<a href="/property/delete/<%= @property.id %>">	<button class="btn btn-default" value="delete">Delete Property</button></a>
</center>

</div>
</div>
</div>